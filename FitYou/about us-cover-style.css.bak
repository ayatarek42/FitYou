 body,
html {
  height: 100%;
  width: 100%
}


.parallax {
  background-image: url('workingout.jpg');
  height: 100%;
  width: 100%;
  /* Parallax scrolling effect */
  background-attachment: fixed; 
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}

